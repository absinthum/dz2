#include <stdio.h>
#include <stdlib.h>
#define MAX 100
int main() {
	int **a, j, i, n, m;
	printf("Uneti dimenzije matrice\n");
	scanf("%d %d", &n, &m);
	a = malloc(n*sizeof(int*));
	printf("Uneti elemente matrice\n");
	for (i = 0; i < n; i++) {
		*(a + i) = malloc(m * sizeof(int));
		for (j = 0; j < m; j++) {
			scanf("%d", *(a + i) + j);
		}
	}
	int p[MAX];
	for (i = 0; i < n; i++) {
		p[i] = 0;
		for (j = 0; j < n; j++) {
			p[i] += *(*(a + i) + j);
		}
	}
	printf("\n");

	for (i = 0; i < n; i++) {
		for (j = 0; j < m; j++) {
			if (p[i] > 0) printf("%d ", *(*(a + i) + j));
		}
		if (p[i] > 0) printf("\n");
	}
	for (i = 0; i < n; i++) {
			free(*(a + i));
	}
	free(a);
	getchar();
  getchar();
}
